<div class="headline">
	<a href="/questions/ask">Ask a Question</a>
</div>
<div class= "container">

	<% @questions.each do |question| %>
	
	<article id= '<%= question.id %>'>
		<div class="votes"><%= question.votes.count %> Votes</div>
		<div class="answers"><%= question.answers.count %> Answers</div>		
		<a href="/questions/<%= question.id %>"	><%= question.title %></a>
	</article>
	<% end %>


</div>